void setup() // This is the function to settup the arduino 
{

    pinMode(2,OUTPUT);  // Initiation of the built-in led on pin 13 as an output
   
    
} // End brakets indicate end of settup function 

void loop()  // Start the main loop
{
    
    digitalWrite(2,HIGH); // Set pin 13 to HIGH (5v) 

} // End the main loop 


